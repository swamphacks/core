            },
            {
              "name": "level",
              "questionType": "select",
              "placeholder": "Select your level",
              "label": "Level of Study",
              "required": true,
              "options": [
                { "label": "Bachelors", "value": "1" },
                { "label": "Masters", "value": "2" }
