    {
      "type": "section",
      "label": "SwampHacks Consent & Agreements",
      "content": [
        {
          "name": "pictureConsent",
          "questionType": "checkbox",
          "label": "I consent to SwampHacks photo and video policy. We will be taking pictures and recording videos throughout the events for promotional use.",
          "required": true,
          "options": [
            {
              "label": "I agree",
              "value": "agree"
            }
          ]
        },
        {
          "name": "inpersonAcknowledgement",
          "questionType": "checkbox",
          "label": "By submitting this application, I recognize that SwampHacks is an in-person only event and my in-person attendance is expected if I am offered acceptance. I also recognize that SwampHacks will be providing free meals but will not be covering transportation costs. ",
          "required": true,
          "options": [
            {
              "label": "I agree",
              "value": "agree"
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "label": "MLH Consent & Agreements",
