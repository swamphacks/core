          "content": [
            {
              "name": "year",
              "questionType": "select",
              "placeholder": "Select your year",
              "label": "Year in College",
              "required": true,
              "hasOther": true,
              "options": [
                {
                  "label": "1st Year",
                  "value": "first_year"
                },
                {
                  "label": "2nd Year",
                  "value": "second_year"
                },
                {
                  "label": "3rd Year",
                  "value": "third_year"
                },
                {
                  "label": "4th Year",
                  "value": "fourth_year"
                },
                {
                  "label": "Graduate Student",
                  "value": "graduate"
